{
  "rules": {
    "curly": [2, "multi-line"],
    "indent": [2, 2],
    "linebreak-style": [2, "unix"],
    "max-len": [1, 80, 2],
    "new-cap": [2, {
    }],
    "no-console": [1],
    "no-use-before-define": [2, "nofunc"],
    "quotes": [2, "single"],
    "semi": [2, "always"]
  },
  "env": {
    "browser": true
  },
  "globals": {
    "angular": false
  }
}
